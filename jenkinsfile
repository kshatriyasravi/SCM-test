pipeline
{
    agent any;
    tools
    {
        maven 'maven 3.6.2'
    }
    stages
    {
        stage('SCM'){
            steps
            {
               sh "git clone https://github.com/kshatriyasravi/maven-web-application.git" 
            }
            
            
        }
        stage('testing')
        {
            steps
            {
                sh 'mvn compile'
            }
        }
    }    
}
